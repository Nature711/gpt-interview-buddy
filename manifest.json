{
    "manifest_version": 3,
    "name": "GPT Interview Buddy",
    "version": "1.0",
    "description": "Your best friend for tech job interview prep, offering tailored advice and feedback.",
    "permissions": ["storage", "activeTab", "tabs", "unlimitedStorage"],
    "icons": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/"],
        "js": ["scripts/initializeContext.js"]
      }
    ]
  }
  